<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>


<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<BorderPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.tidsrejseagentur.Controllers.ControllerSceneLandingPage">
        <top>
            <VBox spacing="10" alignment="CENTER">
                <HBox spacing="15" alignment="CENTER">
                    <Button text="Hovedmenu" onAction="#hovedmenuButton" HBox.hgrow="ALWAYS" />
                    <Button text="Booking" onAction="#bookingButton" HBox.hgrow="ALWAYS" />
                    <Button text="Tidsperioder" onAction="#tidsperioderButton" HBox.hgrow="ALWAYS" />
                </HBox>
                <HBox spacing="15" alignment="CENTER">
                    <Button text="Tidsrejsemaskiner" onAction="#tidsrejsemaskinerButton" HBox.hgrow="ALWAYS" />
                    <Button text="Kundeadministration" onAction="#kundeAdministrationButton" HBox.hgrow="ALWAYS" />
                    <Button text="Guides" onAction="#guidesButton" HBox.hgrow="ALWAYS" />
                </HBox>
            </VBox>
        </top>

<center>
    <Label fx:id="velkommenTekst" text="Hello" />
</center>
</BorderPane>






